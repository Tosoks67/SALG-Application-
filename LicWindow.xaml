<Window x:Class="SALG__Application_.LicWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="License" Height="425" Width="560"
        ResizeMode="NoResize">
    <Grid>
        <TextBox x:Name="txtLicense" IsReadOnly="True" TextWrapping="NoWrap" Margin="10" HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto"/>
    </Grid>
</Window>
